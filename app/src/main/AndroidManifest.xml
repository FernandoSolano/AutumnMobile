<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cr.ac.ucr.paraiso.autumnmobile">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".data.VolleyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ValuationsActivity" />
        <activity android:name=".StatsActivity" />
        <activity android:name=".StatsCognitiveImpairmentActivity" />
        <activity android:name=".StatsDepressiveDisorderActivity" />
        <activity android:name=".StatsEmotionalDisorderActivity" />
        <activity android:name=".StatsMentalDisorderActivity" />
    </application>

</manifest>